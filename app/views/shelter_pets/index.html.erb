<h1>Available Pets</h1>

<% @pets.each do |pet| %>
<img src = "<%= pet.image %>" alt:="<%= pet.name %>" </br>
<h2><%= link_to "#{pet.name}", "/pets/#{pet.id}" %> </h2>
<p> Age: <%= pet.age %> </p>
<p> Sex: <%= pet.sex %> </p>
<% end %>


<a href= "/shelters/<%=@shelter.id%>/pets/new">Create Pet</a>
